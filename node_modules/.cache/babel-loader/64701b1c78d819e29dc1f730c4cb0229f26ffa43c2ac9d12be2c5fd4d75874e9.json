{"ast":null,"code":"import { computed } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRoute } from \"vue-router\";\n\n//获取购物车信息逻辑\nconst useCartEffect = () => {\n  const store = useStore();\n  const route = useRoute();\n  // console.log(route);\n  const shopId = route.params.id;\n  const cartList = store.state.cartList;\n  //计算购物车上的件数\n  const total = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (let i in productList) {\n        const product = productList[i];\n        count += product.count;\n      }\n    }\n    return count;\n  });\n  //计算总计价格\n  const price = computed(() => {\n    const productList = cartList[shopId];\n    let count = 0;\n    if (productList) {\n      for (let i in productList) {\n        const product = productList[i];\n        count += product.count * product.price;\n      }\n    }\n    return count.toFixed(2);\n  });\n  return {\n    price,\n    total\n  };\n};\nexport default {\n  name: \"Cart\",\n  setup() {\n    const {\n      total,\n      price\n    } = useCartEffect();\n    return {\n      total,\n      price\n    };\n  }\n};","map":{"version":3,"names":["computed","useStore","useRoute","useCartEffect","store","route","shopId","params","id","cartList","state","total","productList","count","i","product","price","toFixed","name","setup"],"sources":["F:\\Web2022\\week23-25\\shengxian\\src\\views\\shop\\Cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <div class=\"products\"></div>\r\n    <div class=\"check\">\r\n      <div class=\"check__icon\">\r\n        <img\r\n          src=\"http://www.dell-lee.com/imgs/vue3/basket.png\"\r\n          class=\"check__icon__img\"\r\n        />\r\n        <div class=\"check__icon__tag\">{{ total }}</div>\r\n      </div>\r\n      <div class=\"check__info\">\r\n        总计：<span class=\"check__info__price\">&yen; {{ price }}</span>\r\n      </div>\r\n      <div class=\"check__btn\">去结算</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from \"vue\";\r\nimport { useStore } from \"vuex\";\r\nimport { useRoute } from \"vue-router\";\r\n\r\n//获取购物车信息逻辑\r\nconst useCartEffect = () => {\r\n  const store = useStore();\r\n  const route = useRoute();\r\n  // console.log(route);\r\n  const shopId = route.params.id;\r\n  const cartList = store.state.cartList;\r\n//计算购物车上的件数\r\n  const total = computed(() => {\r\n    const productList = cartList[shopId];\r\n    let count = 0;\r\n    if (productList) {\r\n      for (let i in productList) {\r\n        const product = productList[i];\r\n        count += product.count;\r\n      }\r\n    }\r\n    return count;\r\n  });\r\n//计算总计价格\r\n  const price = computed(() => {\r\n    const productList = cartList[shopId];\r\n    let count = 0;\r\n    if (productList) {\r\n      for (let i in productList) {\r\n        const product = productList[i];\r\n        count += product.count * product.price;\r\n      }\r\n    }\r\n    return count.toFixed(2);\r\n  });\r\n  return {price,total}\r\n};\r\n\r\nexport default {\r\n  name: \"Cart\",\r\n  setup() {\r\n    const {total, price}=useCartEffect()\r\n    return { total, price };\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n\r\n.cart {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n}\r\n.check {\r\n  display: flex;\r\n  height: 0.49rem;\r\n  border-top: 0.01rem solid $content-bgColor;\r\n  line-height: 0.49rem;\r\n  &__icon {\r\n    position: relative;\r\n    width: 0.84rem;\r\n    &__img {\r\n      display: block;\r\n      margin: 0.12rem auto;\r\n      width: 0.28rem;\r\n      height: 0.26rem;\r\n    }\r\n    &__tag {\r\n      position: absolute;\r\n      left: 0.46rem;\r\n      top: 0.04rem;\r\n      padding: 0 0.04rem;\r\n      min-width: 0.2rem;\r\n      height: 0.2rem;\r\n      line-height: 0.2rem;\r\n      border-radius: 0.1rem;\r\n      font-size: 0.12rem;\r\n      text-align: center;\r\n      background-color: $hightlight-fontColor;\r\n      color: #fff;\r\n      transform: scale(0.5, 0.5);\r\n      transform-origin: left center;\r\n    }\r\n  }\r\n  &__info {\r\n    flex: 1;\r\n    color: $content-fontcolor;\r\n    font-size: 0.12rem;\r\n    &__price {\r\n      color: $hightlight-fontColor;\r\n      font-size: 0.18rem;\r\n    }\r\n  }\r\n  &__btn {\r\n    width: 0.98rem;\r\n    background: #4fb0f9;\r\n    color: #fff;\r\n    font-size: 0.14rem;\r\n    text-align: center;\r\n  }\r\n}\r\n</style>"],"mappings":"AAoBA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,QAAO,QAAS,MAAM;AAC/B,SAASC,QAAO,QAAS,YAAY;;AAErC;AACA,MAAMC,aAAY,GAAIA,CAAA,KAAM;EAC1B,MAAMC,KAAI,GAAIH,QAAQ,EAAE;EACxB,MAAMI,KAAI,GAAIH,QAAQ,EAAE;EACxB;EACA,MAAMI,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,EAAE;EAC9B,MAAMC,QAAO,GAAIL,KAAK,CAACM,KAAK,CAACD,QAAQ;EACvC;EACE,MAAME,KAAI,GAAIX,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM,CAAC;IACpC,IAAIO,KAAI,GAAI,CAAC;IACb,IAAID,WAAW,EAAE;MACf,KAAK,IAAIE,CAAA,IAAKF,WAAW,EAAE;QACzB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC,CAAC;QAC9BD,KAAI,IAAKE,OAAO,CAACF,KAAK;MACxB;IACF;IACA,OAAOA,KAAK;EACd,CAAC,CAAC;EACJ;EACE,MAAMG,KAAI,GAAIhB,QAAQ,CAAC,MAAM;IAC3B,MAAMY,WAAU,GAAIH,QAAQ,CAACH,MAAM,CAAC;IACpC,IAAIO,KAAI,GAAI,CAAC;IACb,IAAID,WAAW,EAAE;MACf,KAAK,IAAIE,CAAA,IAAKF,WAAW,EAAE;QACzB,MAAMG,OAAM,GAAIH,WAAW,CAACE,CAAC,CAAC;QAC9BD,KAAI,IAAKE,OAAO,CAACF,KAAI,GAAIE,OAAO,CAACC,KAAK;MACxC;IACF;IACA,OAAOH,KAAK,CAACI,OAAO,CAAC,CAAC,CAAC;EACzB,CAAC,CAAC;EACF,OAAO;IAACD,KAAK;IAACL;EAAK;AACrB,CAAC;AAED,eAAe;EACbO,IAAI,EAAE,MAAM;EACZC,KAAKA,CAAA,EAAG;IACN,MAAM;MAACR,KAAK;MAAEK;IAAK,CAAC,GAACb,aAAa,EAAC;IACnC,OAAO;MAAEQ,KAAK;MAAEK;IAAM,CAAC;EACzB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}