{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { post } from \"../../utils/request\";\nimport { useRoute } from \"vue-router\";\nimport { useCommonCartEffect } from \"../../effects/cartEffects\";\nexport default {\n  name: \"Order\",\n  setup() {\n    const route = useRoute();\n    const shopId = route.params.id;\n    const {\n      calculations\n    } = useCommonCartEffect(shopId);\n    const handleConfirmOrder = async () => {\n      try {\n        const {\n          username,\n          password\n        } = data;\n        if (username === \"\" || password === \"\") {\n          return showToast(\"请填写用户名密码\");\n        }\n        const result = await post(\"/api/user/login\", {\n          username: data.username,\n          password: data.password\n        });\n        // console.log(result);\n        if (result?.errno === 0) {\n          localStorage.isLogin = true;\n          router.push({\n            name: \"Home\"\n          });\n        } else {\n          showToast(\"登录失败\");\n        }\n      } catch (e) {\n        showToast(\"请求失败\");\n      }\n    };\n    const handleCanceOrder = () => {\n      console.log('123');\n    };\n    return {\n      calculations,\n      handleCanceOrder,\n      handleConfirmOrder\n    };\n  }\n};","map":{"version":3,"names":["post","useRoute","useCommonCartEffect","name","setup","route","shopId","params","id","calculations","handleConfirmOrder","username","password","data","showToast","result","errno","localStorage","isLogin","router","push","e","handleCanceOrder","console","log"],"sources":["F:\\Web2022\\week23-25\\shengxian\\src\\views\\orderConfirmation\\Order.vue"],"sourcesContent":["<template>\r\n  <div class=\"order\">\r\n    <div class=\"order__price\">\r\n      实付金额<b>￥{{ calculations.price }}</b>\r\n    </div>\r\n    <div class=\"order__btn\">提交订单</div>\r\n  </div>\r\n  <div class=\"mask\">\r\n    <div class=\"mask__content\">\r\n      <h3 class=\"mask__content__title\">确认要离开收银台？</h3>\r\n      <p class=\"mask__content__desc\">请尽快完成支付，否则将被取消</p>\r\n      <div class=\"mask__content__btns\">\r\n        <div class=\"mask__content__btn mask__content__btn--first\" @click=\"handleCanceOrder\">取消订单</div>\r\n        <div class=\"mask__content__btn mask__content__btn--last\" @click=\"handleConfirmOrder\">确认支付</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport { post } from \"../../utils/request\";\r\nimport { useRoute } from \"vue-router\";\r\nimport { useCommonCartEffect } from \"../../effects/cartEffects\";\r\n\r\nexport default {\r\n  name: \"Order\",\r\n  setup() {\r\n    const route = useRoute();\r\n    const shopId = route.params.id;\r\n    const { calculations } = useCommonCartEffect(shopId);\r\n    const handleConfirmOrder = async () => {\r\n    try {\r\n      const { username, password } = data;\r\n      if (username === \"\" || password === \"\") {\r\n        return showToast(\"请填写用户名密码\");\r\n      }\r\n      const result = await post(\"/api/user/login\", {\r\n        username: data.username,\r\n        password: data.password,\r\n      });\r\n      // console.log(result);\r\n      if (result?.errno === 0) {\r\n        localStorage.isLogin = true;\r\n        router.push({ name: \"Home\" });\r\n      } else {\r\n        showToast(\"登录失败\");\r\n      }\r\n    } catch (e) {\r\n      showToast(\"请求失败\");\r\n    }\r\n  };\r\n    const handleCanceOrder=()=>{\r\n        console.log('123')\r\n    }\r\n    return { calculations,handleCanceOrder,handleConfirmOrder };\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n.order {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  display: flex;\r\n  height: 0.49rem;\r\n  line-height: 0.49rem;\r\n  background: $bgColor;\r\n  &__price {\r\n    flex: 1;\r\n    text-indent: 0.24rem;\r\n    font-size: 0.14rem;\r\n    color: $content-fontcolor;\r\n  }\r\n  &__btn {\r\n    width: 0.98rem;\r\n    background: #4fb0f9;\r\n    color: $bgColor;\r\n    font-size: 0.14rem;\r\n    text-align: center;\r\n  }\r\n}\r\n.mask {\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  bottom: 0;\r\n  top: 0;\r\n  z-index: 1;\r\n  background: rgba(0, 0, 0, 0.5);\r\n  &__content {\r\n    position: absolute;\r\n    top: 50%;\r\n    left: 50%;\r\n    transform: translate(-50%, -50%);\r\n    width: 3rem;\r\n    height: 1.56rem;\r\n    background: #fff;\r\n    border-radius: 0.04rem;\r\n    text-align: center;\r\n    &__title {\r\n      margin: 0.24rem 0 0 0;\r\n      line-height: 0.26rem;\r\n      font-size: 0.18rem;\r\n      color: #333;\r\n    }\r\n    &__desc {\r\n      margin: 0.08rem 0 0 0;\r\n      font-size: 0.14rem;\r\n      color: #666;\r\n    }\r\n    &__btns {\r\n      display: flex;\r\n      margin: 0.24rem 0.58rem 0 0.58rem;\r\n    }\r\n    &__btn {\r\n      flex: 1;\r\n      width: 0.8rem;\r\n      line-height: 0.32rem;\r\n      border-radius: .16rem;\r\n      border-radius: .16rem;\r\n      font-size: .14rem;\r\n      &--first{\r\n        margin-right: .12rem;\r\n        border: .01rem solid #4fb0f9;\r\n        color:#4fb0f9 ;\r\n      }\r\n      &--last{\r\n        margin-left: .12rem;\r\n        background: #4fb0f9;\r\n        color: #fff;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":";AAsBA,SAASA,IAAG,QAAS,qBAAqB;AAC1C,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,mBAAkB,QAAS,2BAA2B;AAE/D,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,EAAE;IACxB,MAAMK,MAAK,GAAID,KAAK,CAACE,MAAM,CAACC,EAAE;IAC9B,MAAM;MAAEC;IAAa,IAAIP,mBAAmB,CAACI,MAAM,CAAC;IACpD,MAAMI,kBAAiB,GAAI,MAAAA,CAAA,KAAY;MACvC,IAAI;QACF,MAAM;UAAEC,QAAQ;UAAEC;QAAS,IAAIC,IAAI;QACnC,IAAIF,QAAO,KAAM,EAAC,IAAKC,QAAO,KAAM,EAAE,EAAE;UACtC,OAAOE,SAAS,CAAC,UAAU,CAAC;QAC9B;QACA,MAAMC,MAAK,GAAI,MAAMf,IAAI,CAAC,iBAAiB,EAAE;UAC3CW,QAAQ,EAAEE,IAAI,CAACF,QAAQ;UACvBC,QAAQ,EAAEC,IAAI,CAACD;QACjB,CAAC,CAAC;QACF;QACA,IAAIG,MAAM,EAAEC,KAAI,KAAM,CAAC,EAAE;UACvBC,YAAY,CAACC,OAAM,GAAI,IAAI;UAC3BC,MAAM