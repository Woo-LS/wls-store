{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Address from \"../../components/Address\";\nimport useCommonAddressEffect from \"../../effects/addressEffect\";\nimport { toRefs } from \"vue\";\nimport { useStore } from \"vuex\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  name: \"MyAddressList\",\n  components: {\n    Address\n  },\n  setup() {\n    const store = useStore();\n    const router = useRouter();\n    const {\n      addressList\n    } = toRefs(store.state);\n    const {\n      getAddressList\n    } = useCommonAddressEffect();\n    getAddressList(true);\n    const handleBackClick = () => {\n      router.back();\n    };\n    const handleAddClick = () => {\n      router.push({\n        name: \"UpsertAddress\"\n      });\n    };\n    const handleUpdateClick = addressId => {\n      router.push(`/upsertAddress/${addressId}`);\n    };\n    return {\n      addressList,\n      handleBackClick,\n      handleAddClick,\n      handleUpdateClick\n    };\n  }\n};","map":{"version":3,"names":["Address","useCommonAddressEffect","toRefs","useStore","useRouter","name","components","setup","store","router","addressList","state","getAddressList","handleBackClick","back","handleAddClick","push","handleUpdateClick","addressId"],"sources":["F:\\Web2022\\shengxian\\src\\views\\myAddressList\\MyAddressList.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"title\">\r\n      <div class=\"iconfont title__back\" @click=\"handleBackClick\">&#xe6db;</div>\r\n      <div class=\"title__text\">管理收货地址</div>\r\n      <div class=\"title__add\" @click=\"handleAddClick\">新建</div>\r\n    </div>\r\n    <Address\r\n      v-for=\"address in addressList\"\r\n      :key=\"address._id\"\r\n      :address=\"address\"\r\n      @click=\"() => handleUpdateClick(address._id)\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Address from \"../../components/Address\";\r\nimport useCommonAddressEffect from \"../../e