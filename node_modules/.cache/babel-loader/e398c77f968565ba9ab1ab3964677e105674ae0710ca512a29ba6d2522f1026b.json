 = await get(\"/api/shop/hot-list\");\r\n    console.log(result);\r\n    if (result?.errno === 0 && result?.data?.length) {\r\n      nearbyList.value = result.data;\r\n    }\r\n  };\r\n  return { nearbyList, getNearbyList };\r\n};\r\n\r\nexport default {\r\n  name: \"Nearby\",\r\n  setup() {\r\n    const { nearbyList, getNearbyList } = useNearbyListEffect();\r\n    getNearbyList();\r\n    return { nearbyList };\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n@import \"../../style/mixins.scss\";\r\n.nearby {\r\n  &__title {\r\n    margin: 0.16rem 0 0.02rem 0;\r\n    font-size: 0.18rem;\r\n    font-weight: normal;\r\n    color: $content-fontcolor;\r\n  }\r\n  &__item {\r\n    display: flex;\r\n    padding-top: 0.12rem;\r\n    &__img {\r\n      margin-right: 0.16rem;\r\n      width: 0.56rem;\r\n      height: 0.56rem;\r\n    }\r\n  }\r\n  &__content {\r\n    flex: 1;\r\n    padding-bottom: 0.12rem;\r\n    border-bottom: 0.01rem solid $content-bgColor;\r\n    &__title {\r\n      line-height: 0.22rem;\r\n      font-size: 0.16rem;\r\n      color: $content-fontcolor;\r\n    }\r\n    &__tags {\r\n      margin-top: 0.08rem;\r\n      line-height: 0.18rem;\r\n      font-size: 0.13rem;\r\n      color: $content-fontcolor;\r\n    }\r\n    &__tag {\r\n      margin-right: 0.16rem;\r\n    }\r\n    &__highlight {\r\n      margin: 0.08rem 0 0 0;\r\n      line-height: 0.18rem;\r\n      font-size: 0.13rem;\r\n      color: #e93b3b;\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAuBA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,GAAE,QAAS,qBAAqB;AAEzC,MAAMC,mBAAkB,GAAIA,CAAA,KAAM;EAChC,MAAMC,UAAS,GAAIH,GAAG,CAAC,EAAE,CAAC;EAC1B,MAAMI,aAAY,GAAI,MAAAA,CAAA,KAAY;IAChC,MAAMC,MAAK,GAAI,MAAMJ,GAAG,CAAC,oBAAoB,CAAC;IAC9CK,OAAO,CAACC,GAAG,CAACF,MAAM,CAAC;IACnB,IAAIA,MAAM,EAAEG,KAAI,KAAM,KAAKH,MAAM,EAAEI,IAAI,EAAEC,MAAM,EAAE;MAC/CP,UAAU,CAACQ,KAAI,GAAIN,MAAM,CAACI,IAAI;IAChC;EACF,CAAC;EACD,OAAO;IAAEN,UAAU;IAAEC;EAAc,CAAC;AACtC,CAAC;AAED,eAAe;EACbQ,IA