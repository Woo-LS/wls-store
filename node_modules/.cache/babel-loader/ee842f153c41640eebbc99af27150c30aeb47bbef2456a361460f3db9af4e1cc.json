{"ast":null,"code":"import { reactive, ref, toRefs } from \"vue\";\nimport { useRoute } from \"vue-router\";\nimport { get } from \"../../utils/request\";\nconst categories = [{\n  name: \"全部商品\",\n  tab: \"all\"\n}, {\n  name: \"秒杀\",\n  tab: \"seckill\"\n}, {\n  name: \"新鲜水果\",\n  tab: \"fruit\"\n}];\nconst useTabEffect = () => {\n  const currentTab = ref(categories[0].tab);\n  const handleTabClick = tab => {\n    currentTab.value = tab;\n  };\n};\nexport default {\n  name: \"Content\",\n  setup() {\n    const data = reactive({\n      contentList: []\n    });\n    const route = useRoute();\n    const getContentData = async tab => {\n      const result = await get(`/api/shop/${route.params.id}/products`, {\n        tab\n      });\n      if (result?.errno === 0 && result?.data?.length) {\n        data.contentList = result.data;\n      }\n    };\n    getContentData(\"all\");\n    const {\n      contentList,\n      currentTab\n    } = toRefs(data);\n    return {\n      contentList,\n      categories,\n      currentTab,\n      handleCategoryClick\n    };\n  }\n};","map":{"version":3,"names":["reactive","ref","toRefs","useRoute","get","categories","name","tab","useTabEffect","currentTab","handleTabClick","value","setup","data","contentList","route","getContentData","result","params","id","errno","length","handleCategoryClick"],"sources":["F:\\Web2022\\week23-25\\shengxian\\src\\views\\shop\\Content.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\">\r\n    <div class=\"category\">\r\n      <div\r\n        :class=\"{\r\n          category__item: true,\r\n          'category__item--active': currentTab === item.tab,\r\n        }\"\r\n        v-for=\"item in categories\"\r\n        :key=\"item.name\"\r\n        @click=\"() => handleCategoryClick(item.tab)\"\r\n      >\r\n        {{ item.name }}\r\n      </div>\r\n    </div>\r\n    <div class=\"product\">\r\n      <div class=\"product__item\" v-for=\"item in contentList\" :key=\"item._id\">\r\n        <img class=\"product__item__img\" :src=\"`${item.imgUrl}`\" />\r\n        <div class=\"product__item__detail\">\r\n          <h4 class=\"product__item__title\">{{ item.name }}</h4>\r\n          <p class=\"product__item__sales\">月售{{ item.sales }}件</p>\r\n          <p class=\"product__item__price\">\r\n            <span class=\"product__item__yen\">&yen;</span>{{ item.price }}\r\n            <span class=\"product__item__origin\">&yen;{{ item.oldPrice }}</span>\r\n          </p>\r\n        </div>\r\n        <div class=\"product__number\">\r\n          <span class=\"product__number__minus\">-</span>\r\n          0\r\n          <span class=\"product__number__plus\">+</span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { reactive, ref, toRefs } from \"vue\";\r\nimport { useRoute } from \"vue-router\";\r\nimport { get } from \"../../utils/request\";\r\n\r\nconst categories = [\r\n  { name: \"全部商品\", tab: \"all\" },\r\n  { name: \"秒杀\", tab: \"seckill\" },\r\n  { name: \"新鲜水果\", tab: \"fruit\" },\r\n];\r\n\r\nconst useTabEffect = () => {\r\n  const currentTab = ref(categories[0].tab);\r\n  const handleTabClick = (tab) => {\r\n    currentTab.value = tab;\r\n  };\r\n};\r\nexport default {\r\n  name: \"Content\",\r\n  setup() {\r\n    const data = reactive({\r\n      contentList: [],\r\n    });\r\n    const route = useRoute();\r\n    const getContentData = async (tab) => {\r\n      const result = await get(`/api/shop/${route.params.id}/products`, {\r\n        tab,\r\n      });\r\n      if (result?.errno === 0 && result?.data?.length) {\r\n        data.contentList = result.data;\r\n      }\r\n    };\r\n\r\n    getContentData(\"all\");\r\n    const { contentList, currentTab } = toRefs(data);\r\n    return { contentList, categories, currentTab, handleCategoryClick };\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"../../style/viriables.scss\";\r\n@import \"../../style/mixins.scss\";\r\n.content {\r\n  display: flex;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 1.5rem;\r\n  bottom: 0.5rem;\r\n  margin-top: 0.16rem;\r\n}\r\n.category {\r\n  overflow-y: scroll;\r\n  height: 100%;\r\n  width: 0.76rem;\r\n  background: $search-bgColor;\r\n  &__item {\r\n    line-height: 0.4rem;\r\n    text-align: center;\r\n    font-size: 0.14rem;\r\n    color: $content-fontcolor;\r\n    &--active {\r\n      background: $bgColor;\r\n    }\r\n  }\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: 0.12rem 0;\r\n    margin: 0 0.16rem;\r\n    border-bottom: 0.01rem solid $content-bgColor;\r\n    &__detail {\r\n      overflow: hidden;\r\n    }\r\n    &__img {\r\n      width: 0.68rem;\r\n      height: 0.68rem;\r\n      margin-right: 0.16rem;\r\n    }\r\n    &__title {\r\n      margin: 0;\r\n      line-height: 0.2rem;\r\n      font-size: 0.14rem;\r\n      color: $content-fontcolor;\r\n      @include ellipsis;\r\n    }\r\n    &__sales {\r\n      margin: 0.06rem 0;\r\n      line-height: 0.16rem;\r\n      font-size: 0.12rem;\r\n      color: $content-fontcolor;\r\n    }\r\n    &__price {\r\n      margin: 0;\r\n      line-height: 0.2rem;\r\n      font-size: 0.14rem;\r\n      color: $hightlight-fontColor;\r\n    }\r\n    &__yen {\r\n      font-size: 0.12rem;\r\n    }\r\n    &__origin {\r\n      margin-left: 0.06rem;\r\n      line-height: 0.2rem;\r\n      font-size: 0.12rem;\r\n      color: $light-fontColor;\r\n      text-decoration: line-through;\r\n    }\r\n    .product__number {\r\n      position: absolute;\r\n      right: 0rem;\r\n      bottom: 0.12rem;\r\n      &__minus,\r\n      &__plus {\r\n        display: inline-block;\r\n        width: 0.2rem;\r\n        height: 0.2rem;\r\n        line-height: 0.16rem;\r\n        border-radius: 50%;\r\n        font-size: 0.2rem;\r\n        text-align: center;\r\n      }\r\n      &__minus {\r\n        border: 0.01rem solid $medium-fontColor;\r\n        color: $medium-fontColor;\r\n        margin-right: 0.05rem;\r\n      }\r\n      &__plus {\r\n        background: $btn-bgColor;\r\n        color: $bgColor;\r\n        margin-left: 0.05rem;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>"],"mappings":"AAoCA,SAASA,QAAQ,EAAEC,GAAG,EAAEC,MAAK,QAAS,KAAK;AAC3C,SAASC,QAAO,QAAS,YAAY;AACrC,SAASC,GAAE,QAAS,qBAAqB;AAEzC,MAAMC,UAAS,GAAI,CACjB;EAAEC,IAAI,EAAE,MAAM;EAAEC,GAAG,EAAE;AAAM,CAAC,EAC5B;EAAED,IAAI,EAAE,IAAI;EAAEC,GAAG,EAAE;AAAU,CAAC,EAC9B;EAAED,IAAI,EAAE,MAAM;EAAEC,GAAG,EAAE;AAAQ,CAAC,CAC/B;AAED,MAAMC,YAAW,GAAIA,CAAA,KAAM;EACzB,MAAMC,UAAS,GAAIR,GAAG,CAACI,UAAU,CAAC,CAAC,CAAC,CAACE,GAAG,CAAC;EACzC,MAAMG,cAAa,GAAKH,GAAG,IAAK;IAC9BE,UAAU,CAACE,KAAI,GAAIJ,GAAG;EACxB,CAAC;AACH,CAAC;AACD,eAAe;EACbD,IAAI,EAAE,SAAS;EACfM,KAAKA,CAAA,EAAG;IACN,MAAMC,IAAG,GAAIb,QAAQ,CAAC;MACpBc,WAAW,EAAE;IACf,CAAC,CAAC;IACF,MAAMC,KAAI,GAAIZ,QAAQ,EAAE;IACxB,MAAMa,cAAa,GAAI,MAAOT,GAAG,IAAK;MACpC,MAAMU,MAAK,GAAI,MAAMb,GAAG,CAAE,aAAYW,KAAK,CAACG,MAAM,CAACC,EAAG,WAAU,EAAE;QAChEZ;MACF,CAAC,CAAC;MACF,IAAIU,MAAM,EAAEG,KAAI,KAAM,KAAKH,MAAM,EAAEJ,IAAI,EAAEQ,MAAM,EAAE;QAC/CR,IAAI,CAACC,WAAU,GAAIG,MAAM,CAACJ,IAAI;MAChC;IACF,CAAC;IAEDG,cAAc,CAAC,KAAK,CAAC;IACrB,MAAM;MAAEF,WAAW;MAAEL;IAAW,IAAIP,MAAM,CAACW,IAAI,CAAC;IAChD,OAAO;MAAEC,WAAW;MAAET,UAAU;MAAEI,UAAU;MAAEa;IAAoB,CAAC;EACrE;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}